{
  "name": "genebean-zabbixagent",
  "version": "2.0.0",
  "author": "Gene Liverman",
  "summary": "This module manages the Zabbix Agent on Windows or Linux.",
  "license": "BSD-3-Clause",
  "source": "https://github.com:genebean/genebean-zabbixagent.git",
  "project_page": "https://github.com/genebean/genebean-zabbixagent",
  "issues_url": "https://github.com/genebean/genebean-zabbixagent/issues",
