failovermethod=priority
enabled=0
gpgkey=http://download.fedoraproject.org/pub/epel/RPM-GPG-KEY-EPEL-<%= @operatingsystemmajrelease %>
gpgcheck=1

[epel-testing-source]
name=Extra Packages for Enterprise Linux <%= @operatingsystemmajrelease %> - Testing - $basearch - Source
#baseurl=http://download.fedoraproject.org/pub/epel/testing/<%= @operatingsystemmajrelease %>/SRPMS
